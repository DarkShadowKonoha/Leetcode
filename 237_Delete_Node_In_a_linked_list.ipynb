   "source": [
    "# Instead of deleting the node, we are just changing the values of \n",
    "# node with the next node\n",
    "# O(n) time complexity\n",
    "class Solution:\n",
    "    def deleteNode(self, node):\n",
    "        node.val = node.next.val\n",
    "        node.next = node.next.next"
   ]
  }
